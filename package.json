{
  "name": "mgtab",
  "displayName": "",
  "description": "a manganese open tab file",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
		{
			"command": "extension.openFile",
			"title": "Open File"
		}],
		"views": {
			"explorer": [
					{
							"id": "fileTreeView",
							"name": "MgTabFile"
					}
			]
		},
		"activationEvents": [
			"onCommand:extension.openFile",
			"onView:fileTreeView"
		]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
